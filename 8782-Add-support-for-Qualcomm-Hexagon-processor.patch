+						e.printStackTrace();
+					}
+				}
+			}
+
+			AddressSet disassembled = dis.disassemble(addr, null, false);
+			if (!disassembled.contains(addr)) {
+				return false;
+			}
+
+			AutoAnalysisManager.getAnalysisManager(program).codeDefined(disassembled);
+		}
+
+		return true;
+	}
+}
