+          [ <PEQUALS> ( <PIDENTIFIER> | <PSTRING_LITERAL> | <PINTEGER_LITERAL> | <PCOMMA>) ] )
   {
     if (id.image.equals("pack") && ds1 != null) {
         Token newPackVal = ds1;
