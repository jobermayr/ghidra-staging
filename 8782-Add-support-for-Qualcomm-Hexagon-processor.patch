+    imm_20_21u        = (20,21)
+    imm_20            = (20,20) signed
+
+    imm_19_20         = (19,20) signed
+    imm_19            = (19,19) signed
+
+    imm_18_21         = (18,21)
+    imm_18_20         = (18,20)
+    imm_18_19         = (18,19)
+
+    imm_17_27         = (17,27)
+
+    imm_16_27         = (16,27) signed
+    imm_16_25         = (16,25) signed
+    imm_16_24         = (16,24) signed
+    imm_16_24u        = (16,24)
