@@ -1368,6 +1368,8 @@ TOKEN :
     |
     <PCOMMA : ",">
     |
+    <PEQUALS : "=">
+    |
     <PINTEGER_LITERAL : 
         <PDECIMAL_LITERAL> (  "ull" | "ULL" | "ul" | "UL" | "ll" | "LL" | "l" | "L" | "U" | "u")?
         |
@@ -1926,8 +1928,9 @@ void PragmaSpecifier() : {
 {
   LOOKAHEAD(3)
   <POPEN> PragmaSpecifier() <PCLOSE> |
